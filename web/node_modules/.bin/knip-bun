#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/alistair/mochi/apps/login/web/node_modules/.pnpm/knip@5.64.2_@types+node@24.7.0_typescript@5.9.3/node_modules/knip/bin/node_modules:/home/alistair/mochi/apps/login/web/node_modules/.pnpm/knip@5.64.2_@types+node@24.7.0_typescript@5.9.3/node_modules/knip/node_modules:/home/alistair/mochi/apps/login/web/node_modules/.pnpm/knip@5.64.2_@types+node@24.7.0_typescript@5.9.3/node_modules:/home/alistair/mochi/apps/login/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/alistair/mochi/apps/login/web/node_modules/.pnpm/knip@5.64.2_@types+node@24.7.0_typescript@5.9.3/node_modules/knip/bin/node_modules:/home/alistair/mochi/apps/login/web/node_modules/.pnpm/knip@5.64.2_@types+node@24.7.0_typescript@5.9.3/node_modules/knip/node_modules:/home/alistair/mochi/apps/login/web/node_modules/.pnpm/knip@5.64.2_@types+node@24.7.0_typescript@5.9.3/node_modules:/home/alistair/mochi/apps/login/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bun" ]; then
  exec "$basedir/bun"  "$basedir/../knip/bin/knip-bun.js" "$@"
else
  exec bun  "$basedir/../knip/bin/knip-bun.js" "$@"
fi
